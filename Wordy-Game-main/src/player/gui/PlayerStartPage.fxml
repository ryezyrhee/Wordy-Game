<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.WordyClient">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" />
      <ImageView fitHeight="560.0" fitWidth="779.0" pickOnBounds="true" preserveRatio="true">
         <image>
                  <Image url="@background/startpage.jpg" />
               </image>
            </ImageView>
      <ImageView fitHeight="215.0" fitWidth="504.0" layoutX="143.0" layoutY="159.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@background/wordy-.png" />
         </image>
      </ImageView>
      <Button fx:id="startButton" layoutX="285.0" layoutY="327.0" mnemonicParsing="false" onAction="#startPressed" prefHeight="49.0" prefWidth="219.0" style="-fx-background-color: #6fe8b5; -fx-background-radius: 15px;" text="Start" textFill="WHITE">
         <font>
            <Font name="Rockwell Bold" size="36.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="720.0" layoutY="14.0" mnemonicParsing="false" onAction="#closePressed" style="-fx-background-color: #6fe8b5; -fx-background-radius: 10px;" text="x" textFill="WHITE">
         <font>
            <Font name="Rockwell" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
